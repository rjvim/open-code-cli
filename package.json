{
  "name": "open-code-cli",
  "version": "0.1.1",
  "description": "CLI tool for synchronizing and contributing to component-based codebases",
  "license": "MIT",
  "type": "module",
  "bin": {
    "open-code": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "ncc build ./src/index.ts -w -o dist/",
    "build": "ncc build ./src/index.ts -o ./dist/ --minify --no-cache --no-source-map-register",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "canary": "pnpm run build && npx auto canary --force"
  },
  "devDependencies": {
    "@auto-it/all-contributors": "^11.3.0",
    "@auto-it/conventional-commits": "^11.3.0",
    "@auto-it/npm": "^11.3.0",
    "@auto-it/omit-commits": "^11.3.0",
    "@auto-it/omit-release-notes": "^11.3.0",
    "@auto-it/released": "^11.3.0",
    "@types/cross-spawn": "^6.0.6",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.5",
    "@vercel/ncc": "0.38.1",
    "auto": "^11.3.0",
    "commander": "^11.1.0",
    "cross-spawn": "^7.0.6",
    "execa": "^9.5.2",
    "fs-extra": "^11.3.0",
    "kleur": "^4.1.5",
    "ora": "^8.2.0",
    "picocolors": "^1.1.1",
    "rimraf": "^5.0.5",
    "tar": "^7.4.3",
    "type-fest": "^4.37.0",
    "typescript": "^5.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^1.2.1"
  },
  "packageManager": "pnpm@10.6.2"
}
